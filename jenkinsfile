node{
   sh "pwd>current_path"
   echo '1'
   env.WORKSPACE = readFile 'current_path'
   echo '2'
   env.WORKSPACE = env.WORKSPACE.trim()
   echo '3'
   
   stage "checkout"
   checkout scm
   
   stage 'build'
   sh 'mvn clean package -DskipTests -Dbuild.number=01 -repo.local='+ env.WORKSPACE +'/.m2/repository'
    
       }
